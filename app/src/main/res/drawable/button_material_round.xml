<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3000pt" />
            <stroke android:width="1dip" android:color="#2A526B" />
            <gradient android:angle="-90" android:startColor="#224156" android:endColor="#3F5A6C"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3000pt" />
            <stroke android:width="1dip" android:color="#3F5A6C" />
            <solid android:color="#506879"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3000pt" />
            <stroke android:width="1dip" android:color="#24455B" />
            <gradient android:angle="-90" android:startColor="#1D3649" android:endColor="#224156" />
        </shape>
    </item>
</selector>