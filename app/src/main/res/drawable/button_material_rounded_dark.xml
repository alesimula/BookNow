<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="10dp" />
            <gradient android:angle="-90" android:startColor="#957475" android:endColor="#9F7B7C"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="10dp" />
            <gradient android:angle="-90" android:startColor="#8C6E6F" android:endColor="#987576"  />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="10dp" />
            <gradient android:angle="-90" android:startColor="#896768" android:endColor="#956F70" />
        </shape>
    </item>
</selector>